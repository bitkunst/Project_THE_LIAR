{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true,
		"browser": true
	},
	"plugins": ["prettier"],
	"extends": ["eslint:recommended", "plugin:prettier/recommended"],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"ignorePatterns": ["dist/", "node_modules/"],
	"rules": {
		"prettier/prettier": "warn",
		"no-unused-vars": "warn"
	}
}
